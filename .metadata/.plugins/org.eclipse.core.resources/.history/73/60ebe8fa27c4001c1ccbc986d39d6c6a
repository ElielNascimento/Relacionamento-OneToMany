package com.elieldesenvolvimento.escola.escola;

public class Escola {

	private Long id;
	private String nome;
	private String cpf;

	public Escola() {
	}

	public Escola(String nome, String cpf) {
		super();
		this.nome = nome;
		this.cpf = cpf;
	}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public String getCpf() {
		return cpf;
	}

	public void setCpf(String cpf) {
		this.cpf = cpf;
	}

	public Long getId() {
		return id;
	}

}
